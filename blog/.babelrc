{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory":"lib",
        "style": true
      }
    ],[
      "module-resolver",
      {
        "alias": {
          "@/components": "./components",
          "@/pages": "./pages",
          "@/public": "./public",
          "@/utils": "./utils",
          "@/api": "./api"
        }
      }
    ]
  ]
}